[flake8]
select = B,B9,C,D,DAR,E,F,N,RST,S,W
ignore = E203,E501,RST201,RST203,RST301,W503,D107,B903
max-line-length = 120
max-complexity = 10
docstring-convention = google
per-file-ignores = tests/*:S101,D103,B950 src/tink_fpe/proto/*:B950,E712
rst-roles = class,const,func,meth,mod,ref
rst-directives = deprecated
